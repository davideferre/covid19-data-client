<select class="form-select" aria-label="Dati regionali" {{on "change" this.onRegionChange}}>
    <option value="-1">-Seleziona regione-</option>
    {{#each this.regionsOptions as |region|}}
        <option value={{region.value}} selected={{if (eq @region region.value) true false}}>{{region.label}}</option>
    {{/each}}
</select>